<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		.TicTacToeBoard {
			margin-left: auto;
    		margin-right: auto;
			border-style: solid;
			border-width: medium;
			border-color: #aaaaaa;
		}
	</ui:style>

	<g:DecoratorPanel>
		<g:VerticalPanel addStyleNames="{style.TicTacToeBoard}">
			<g:HorizontalPanel>
				<g:Image ui:field="cell0" />
				<g:Image ui:field="cell1" />
				<g:Image ui:field="cell2" />
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="cell3" />
				<g:Image ui:field="cell4" />
				<g:Image ui:field="cell5" />
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="cell6" />
				<g:Image ui:field="cell7" />
				<g:Image ui:field="cell8" />
			</g:HorizontalPanel>
			<g:HTML ui:field="boardContainer"></g:HTML>
			<!--<g:FlexTable ui:field="contactsTable" addStyleNames="{style.contactsViewContactsFlexTable}"/> -->
		</g:VerticalPanel>
	</g:DecoratorPanel>
</ui:UiBinder> 